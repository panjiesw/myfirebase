{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "importHelpers": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "outDir": "build",
    "paths": {
      "*": [
        "*"
      ],
      "nedb-promise": [
        "typings/nedb-promise.d.ts"
      ],
      "tap": [
        "typings/tap.d.ts"
      ],
      "dotenv-expand": [
        "typings/dotenv-expand.d.ts"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
