  "scripts": {
    "build:server": "npm run clean && tsc -p server",
    "clean": "del build",
    "start:server": "ts-node -r tsconfig-paths/register server/index.ts",
    "test:server": "tap --node-arg=-r --node-arg=tsconfig-paths/register ./server/**/__tests__/*.spec.ts",
    "watch:server": "tsc -p server --watch"
  },
